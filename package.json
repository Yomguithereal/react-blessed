{
  "name": "react-blessed",
  "version": "0.7.0",
  "description": "A react renderer for blessed.",
  "main": "src/reconciler.js",
  "scripts": {
    "build": "tsc",
    "clean": "del '{src,test}/**/*.js'",
    "demo": "node run.js",
    "format": "prettier '{examples,run,src,test}/**/*.{ts,tsx}' --write",
    "prepublish": "npm run build",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yomguithereal/react-blessed.git"
  },
  "keywords": [
    "blessed",
    "react",
    "renderer",
    "cli",
    "interface"
  ],
  "author": "yomguithereal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yomguithereal/react-blessed/issues"
  },
  "browserslist": "maintained node versions",
  "homepage": "https://github.com/yomguithereal/react-blessed#readme",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/register": "^7.9.0",
    "@types/blessed": "^0.1.17",
    "@types/lodash": "^4.14.152",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.4",
    "@types/react": "^16.9.35",
    "@types/react-motion": "0.0.29",
    "@types/react-reconciler": "^0.18.0",
    "ava": "^3.8.2",
    "blessed": "^0.1.81",
    "del-cli": "^3.0.1",
    "invariant": "^2.2.0",
    "lodash": "^4.x.x",
    "mocha": "^4.0.1",
    "neo-blessed": "^0.2.0",
    "node-pty": "^0.9.0",
    "prettier": "^2.0.5",
    "react-motion": "^0.5.2",
    "through2": "^3.0.1",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  },
  "peerDependencies": {
    "blessed": "^0.1.81",
    "react": "^16.13.1",
    "react-devtools-core": "^4.7.0",
    "ws": "^7.3.0"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-reconciler": "^0.25.1"
  },
  "ava": {
    "tap": true,
    "files": [
      "test/**/*.test.{ts,tsx}"
    ],
    "extensions": [
      "ts",
      "tsx"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
